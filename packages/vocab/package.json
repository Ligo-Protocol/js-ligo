{
  "name": "@js-ligo/vocab",
  "version": "0.1.0",
  "description": "Typescript types for Ligo vocabulary",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "pnpm build:clean && pnpm build:types && pnpm build:js",
    "lint": "eslint src --fix",
    "prepare": "pnpm build"
  },
  "dependencies": {
    "schema-dts": "^1.1.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
